
import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter a number:");
        int i= sc.nextInt();
        if (i%2==0) {
            System.out.println("Even number");
        }
        else {
            System.out.println("Odd Number");
        }
    }
}
