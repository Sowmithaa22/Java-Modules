/* Inheritance and Method-Overriding

class parent {
    // code
}

class child extends parent {
    // code
    // addn
}
*/
class wolters {
    void id() {
        System.out.println("Every employee gets a unique ID");
    }
}

class abc extends wolters {
    void l2() {
        System.out.println("abc is part of L2 team");
    }
}

class xyz extends wolters {
    void intern() {
        System.out.println("xyz is an Intern in the product support team.");
    }
    
    @Override void id() {
        System.out.println("This ID for interns is generated by PersolKelly!");
    }
}

public class Main {
    public static void main(String[] args) {
        abc a= new abc();
        a.id();
        a.l2();
        
        xyz s= new xyz();
        s.id();
    }
    
}

