public class Main {
    public static void main(String[] args) {
        
        Student s= new Student();
        
        s.name= "abc";
        s.rollno= 1234;
        
        s.display();
        
    }
}

class Student {
    String name;
    int rollno;
    
    void display() {
        System.out.println(name + " " + rollno);
    }
    
}
